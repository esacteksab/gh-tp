exec gh-tp -o plan.out -m plan.md -b terraform
exists plan.out
exists plan.md
! exists tfplan
! exists tfplan.md
stdout '✔  Plan Created...'\n'✔  Markdown Created...'
cmp plan.md tfgolden.md

-- .tp.toml --
binary = 'tofu'
planFile = 'tfplan'
mdFile = 'tfplan.md'
verbose = false

